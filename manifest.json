{
  "name": "VIP Bot Agent",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "bitcoin.co.id bot agent",
  "homepage_url": "http://imgmin.co",
  "icons": {
    "16": "icons/icon16x.png",
    "48": "icons/icon48x.png",
    "128": "icons/icon128x.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19x.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "tabs",
    "https://vip.bitcoin.co.id/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://vip.bitcoin.co.id/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ],
  "web_accessible_resources" : ["/js/EA.js","/js/TA.js"]
}